{
  "name": "cf-worker-api",
  "type": "module",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev2": "wrangler -c wrangler.jsonc dev",
    "build": "vite build",
    "test": "vitest",
    "preview": "npm run build && vite preview",
    "deploy": "npm run deploy:sync && npm run deploy:api",
    "deploy:sync": "npm run build && wrangler -c livestore-sync/wrangler.jsonc deploy",
    "deploy:api": "npm run build && wrangler -c api/wrangler.jsonc deploy",
    "lint:fix": "biome check --fix"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.9.0",
    "@livestore/sync-cf": "^0.3.1",
    "@noble/ciphers": "^1.3.0",
    "hono": "^4.9.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@cloudflare/containers": "^0.0.19",
    "@cloudflare/workers-types": "^4.20250620.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vitest": "^2.1.8",
    "wrangler": "^4.26.0"
  }
}
